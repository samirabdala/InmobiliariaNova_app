        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:text="fin"
        android:textColor="@color/material_dynamic_neutral_variant30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tvFechaFin1"
        app:layout_constraintTop_toBottomOf="@+id/tvFechaInicio"
        app:layout_constraintWidth_percent="0.75" />

    <TextView
        android:id="@+id/tvFechaInicio1"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:text="Fecha inicio"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvNombreCompletoDetalleContrato"
        app:layout_constraintWidth_percent="0.25" />

    <TextView
        android:id="@+id/tvFechaFin1"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:text="Fecha fin"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvFechaInicio1"
        app:layout_constraintWidth_percent="0.25" />

    <Button
        android:id="@+id/btPagos"
        android:layout_width="343dp"
        android:layout_height="49dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="15dp"
        android:background="@color/light_blue"
        android:backgroundTint="@color/light_blue"
        android:text="Ver pagos"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvDescripcionDetalle"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tvMontoDetalleContrato1"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:text="Monto"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvFechaFin1" />

    <TextView
        android:id="@+id/tvMontoDetalleContrato"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="30dp"
        android:text="monto"
        app:layout_constraintStart_toEndOf="@+id/tvMontoDetalleContrato1"
        app:layout_constraintTop_toBottomOf="@+id/tvFechaFin" />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>
