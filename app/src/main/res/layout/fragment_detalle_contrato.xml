        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvDescripcion1"
        app:layout_constraintWidth_percent="0.75" />

    <TextView
        android:id="@+id/tvDescripcion1"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="Descripci贸n"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvObservacionesDetalle"
        app:layout_constraintWidth_percent="0.25" />

    <TextView
        android:id="@+id/tvObservacionesDetalle"
        android:layout_width="360dp"
        android:layout_height="90dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="observacion"
        android:textColor="@color/material_dynamic_neutral_variant30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvObservaciones"
        app:layout_constraintWidth_percent="0.75" />

    <TextView
        android:id="@+id/tvObservaciones"
        android:layout_width="0dp"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:text="Observaci贸n"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvMontoDetalleContrato1"
        app:layout_constraintWidth_percent="0.25" />

    <TextView
        android:id="@+id/tvFechaInicio"
        android:layout_width="205dp"
        android:layout_height="26dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:text="inicio"
        android:textColor="@color/material_dynamic_neutral_variant30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tvFechaInicio1"
        app:layout_constraintTop_toBottomOf="@+id/tvNombreDetalle"
        app:layout_constraintWidth_percent="0.75" />

    <TextView
        android:id="@+id/tvNombreDetalle"
        android:layout_width="205dp"
        android:layout_height="26dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        android:text="inquilino"
        android:textColor="@color/material_dynamic_neutral_variant30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/tvNombreCompletoDetalleContrato"
        app:layout_constraintTop_toBottomOf="@+id/btCargarImg"
        app:layout_constraintWidth_percent="0.75" />

